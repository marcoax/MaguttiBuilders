{
    "name": "magutti/magutti-builders",
    "description": "Query Builder Helpers",
    "homepage": "https://github.com/marcoax/MaguttiBuilders",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "marco asperti",
            "email": "marcoasperti@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5|~5.6|~5.7|~5.8|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5|~3.6|~3.7|~3.8|^4.0"
    },
    "autoload": {
        "psr-4": {
            "magutti\\MaguttiBuilders\\": "src"
        }
